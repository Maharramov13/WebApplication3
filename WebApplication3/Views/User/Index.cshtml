@{
    var user = ViewBag.User as WebApplication3.Controllers.User;
}
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Surname</th>
            <th scope="col">Age</th>
            <th scope="col">Country</th>
            <th scope="col">PhoneNumber</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">1</th>
            @if (@ViewBag.User !=null)
            {
                <td>@user.Name</td>
                <td>@user.Surname</td>
                <td>@user.Age</td>
                <td>@user.Country</td>
                <td>@user.PhoneNumber</td>
            }
            else
            {
                <td colspan="5">No user data available</td>
            }
        
        </tr>
    
    </tbody>
</table>
